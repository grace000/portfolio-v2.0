{
  "name": "gatsby-portfolio-v2.0",
  "description": "Contentful Gatsby Portfolio with Blog Pages",
  "version": "1.0.0",
  "author": "Tiffani Elmore",
  "dependencies": {
    "core-js": "^3.6.5",
    "gatsby": "^2.21.21",
    "gatsby-cli": "^2.12.15",
    "gatsby-image": "^2.4.3",
    "gatsby-link": "^2.4.2",
    "gatsby-plugin-manifest": "^2.4.1",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sass": "^2.3.1",
    "gatsby-plugin-sharp": "^2.6.2",
    "gatsby-remark-images": "^3.3.2",
    "gatsby-source-contentful": "^2.3.3",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.5.2",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "react-images": "^1.0.0",
    "rebass": "^4.0.7",
    "sharp": "^0.25.2",
    "webpack": "^4.43.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "chalk": "^2.4.2",
    "contentful-import": "^7.7.14",
    "eslint": "^7.0.0",
    "eslint-plugin-react": "^7.19.0",
    "gh-pages": "^2.2.0",
    "inquirer": "^7.1.0",
    "prettier": "^2.0.5",
    "rimraf": "^2.6.3",
    "yargs-parser": "^18.1.3"
  },
  "keywords": [
    "gatsby",
    "contentful"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "dev": "gatsby develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md' 'bin/*.js'",
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix *.js bin/*.js",
    "postinstall": "node ./bin/hello.js",
    "setup": "node ./bin/setup.js",
    "cleanup-repository": "rimraf bin contentful && yarn remove contentful-import chalk inquirer rimraf "
  }
}
